<% if(success.length!=0){%>
    <div class="alert alert-success fade show alert-dismissible col-6 m-auto" role="alert">
        <%= success %>
        <button
        class="btn-close"
        data-bs-dismiss="alert"
        aria-label="Close"
        ></button>
    </div>
<%}%>

<% if(error.length!=0){%>
    <div class="alert alert-danger fade show alert-dismissible col-6 m-auto" role="alert">
        <%= error %>
        <button
        class="btn-close"
        data-bs-dismiss="alert"
        aria-label="Close"
        ></button>
    </div>
<%}%>


